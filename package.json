{
  "name": "react-playgroud",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:4000",
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "babel": {
    "presets": [
      "env",
      "react-app"
    ],
    "plugins": [
      "dynamic-import-node"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "serve:dev": "cross-env NODE_ENV=development nodemon -- --inspect",
    "serve:prod": "cross-env NODE_ENV=production node server/index.js",
    "build:client": "react-scripts build && cpr build/index.html build/404.html",
    "build:server": "cross-env NODE_ENV=production babel src --out-dir babel-src --copy-files --ignore '**/*.test.js' --plugins transform-es2015-modules-commonjs",
    "prebuild": "npm run clean",
    "build": "yarn build:client && yarn build:server",
    "test": "react-scripts test --env=jsdom",
    "test:debug": "react-scripts --inspect-brk test --runInBand --env=jsdom",
    "clean": "rimraf babel-src build",
    "precommit": "lint-staged",
    "analyze": "source-map-explorer build/static/js/main.*",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.18.2",
    "bootstrap": "4.0.0",
    "compression": "^1.7.1",
    "ejs": "2.5.7",
    "express": "4.16.2",
    "firebase": "4.9.0",
    "globby": "^7.1.1",
    "helmet": "^3.10.0",
    "history": "4.7.2",
    "ignore-styles": "^5.0.1",
    "jsonwebtoken": "^8.1.1",
    "lodash": "4.17.4",
    "md5-file": "^3.2.3",
    "morgan": "1.9.0",
    "multer": "^1.3.0",
    "nodemailer": "^4.4.2",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth": "^1.0.0",
    "passport-jwt": "^3.0.1",
    "passport-local": "^1.0.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-helmet": "5.2.0",
    "react-loadable": "^5.3.1",
    "react-redux": "5.0.6",
    "react-router-dom": "4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "1.1.0",
    "react-transition-group": "2.2.1",
    "redux": "3.7.2",
    "redux-form": "7.2.1",
    "redux-thunk": "2.2.0",
    "sequelize": "^4.32.1",
    "sqlite3": "^3.1.13"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "concurrently": "^3.5.1",
    "cpr": "^3.0.1",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "expect": "22.1.0",
    "husky": "0.14.3",
    "lint-staged": "6.0.1",
    "nock": "9.1.6",
    "nodemon": "^1.14.11",
    "prettier": "1.10.2",
    "react-test-renderer": "16.2.0",
    "redux-immutable-state-invariant": "2.1.0",
    "redux-logger": "3.0.6",
    "redux-mock-store": "1.4.0",
    "source-map-explorer": "1.5.0",
    "ts-node": "^4.1.0"
  }
}
